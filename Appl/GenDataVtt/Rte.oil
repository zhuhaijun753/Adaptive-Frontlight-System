   TASK SchM_Task {
      ACTIVATION = 1:"@RO@";
      PRIORITY = 6:"@RO@";
      SCHEDULE = FULL:"@RO@";
// TIMING_PROTECTION = FALSE|TRUE; (AUTOSAR OS only)
      AUTOSTART = FALSE:"@RO@";
      EVENT = Rte_Ev_Cyclic2_SchM_Task_0_10ms:"@RO@";
      EVENT = Rte_Ev_Cyclic2_SchM_Task_0_20ms:"@RO@";
      EVENT = Rte_Ev_Run_Fee_Fee_MainFunction:"@RO@";
   }:"@RO@@NR@";

   ALARM Rte_Al_TE2_SchM_Task_0_10ms {
      COUNTER = SystemTimer;
      ACTION = SETEVENT
      {
         TASK = SchM_Task;
         EVENT = Rte_Ev_Cyclic2_SchM_Task_0_10ms;
      }:"@ROC@";
      AUTOSTART = FALSE:"@RO@";
   }:"@RO@@NR@";

   ALARM Rte_Al_TE2_SchM_Task_0_20ms {
      COUNTER = SystemTimer;
      ACTION = SETEVENT
      {
         TASK = SchM_Task;
         EVENT = Rte_Ev_Cyclic2_SchM_Task_0_20ms;
      }:"@ROC@";
      AUTOSTART = FALSE:"@RO@";
   }:"@RO@@NR@";

   ALARM Rte_Al_TE_Fee_Fee_MainFunction {
      COUNTER = SystemTimer;
      ACTION = SETEVENT
      {
         TASK = SchM_Task;
         EVENT = Rte_Ev_Run_Fee_Fee_MainFunction;
      }:"@ROC@";
      AUTOSTART = FALSE:"@RO@";
   }:"@RO@@NR@";

   EVENT Rte_Ev_Cyclic2_SchM_Task_0_10ms {
      MASK = AUTO;
   }:"@ROC@@NR@";

   EVENT Rte_Ev_Cyclic2_SchM_Task_0_20ms {
      MASK = AUTO;
   }:"@ROC@@NR@";

   EVENT Rte_Ev_Run_Fee_Fee_MainFunction {
      MASK = AUTO;
   }:"@ROC@@NR@";
